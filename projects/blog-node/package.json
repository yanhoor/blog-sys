{
  "name": "blog-node",
  "version": "1.0.0",
  "description": "",
  "nodemonConfig": {
    "watch": [
      "src/**/"
    ]
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.vue,.ts,.tsx --fix --quiet ./",
    "generate": "npx prisma generate",
    "push": "npx prisma db push",
    "prisma": "dotenv -e .env.development -- npm run generate && dotenv -e .env.development -- npm run push",
    "docker": "dotenv -e .env.docker -- npm run prisma && dotenv -e .env.docker -- nodemon src/index.js",
    "dev": "dotenv -e .env.development -- nodemon src/index.js",
    "prod": "dotenv -e .env.production -- npm run prisma && dotenv -e .env.production -- pm2 start ecosystem.config.js",
    "dev_prod": "dotenv -e .env.dev_prod -- npm run prisma && dotenv -e .env.dev_prod -- nodemon src/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@prisma/client": "^5.15.1",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.15.1",
    "redis": "^4.6.13"
  },
  "dependencies": {
    "ali-oss": "^6.20.0",
    "axios": "^1.6.8",
    "config-lite": "^4.0.0",
    "dayjs": "^1.11.10",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.2",
    "koa-body": "^6.0.1",
    "koa-etag": "^4.0.0",
    "koa-jwt": "^4.0.4",
    "koa-mount": "^4.0.0",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "log4js": "^6.9.1",
    "md5-file": "^5.0.0",
    "mime": "^3.0.0",
    "sharp": "^0.33.4",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1"
  }
}
